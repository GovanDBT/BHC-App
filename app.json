{
  "expo": {
    "name": "BHC-App",
    "slug": "BHC-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/bhc-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/front.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
